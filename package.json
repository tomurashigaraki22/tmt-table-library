{
  "name": "tmt-table-library",
  "version": "1.0.3",
  "description": "A modern table library for React with built-in sorting, filtering, and custom styling support.",
  "main": "./dist/modern-react-table.umd.js",
  "module": "./dist/modern-react-table.es.js",
  "exports": {
    ".": {
      "import": "./dist/modern-react-table.es.js",
      "require": "./dist/modern-react-table.umd.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "table",
    "sorting",
    "filtering",
    "library",
    "modern",
    "responsive",
    "styled-components"
  ],
  "author": "Raphael Tomiwa <devtomiwa9@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "vite": "^6.0.6"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
